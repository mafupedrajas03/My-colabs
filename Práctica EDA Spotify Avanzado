      "source": [
        "import plotly.express as px\n",
        "\n",
        "# Contar el n√∫mero de canciones lanzadas por cada artista\n",
        "artist_counts = df_spotify_2023['artist(s)_name'].value_counts()\n",
        "\n",
        "# Seleccionar los 10 artistas principales\n",
        "top_artists = artist_counts.head(10)\n",
        "\n",
        "# Crear el gr√°fico de barras con Plotly\n",
        "fig = px.bar(\n",
        "    x=top_artists.index,\n",
        "    y=top_artists.values,\n",
        "    title='Top 10 Artistas con M√°s √âxitos en 2023',\n",
        "    labels={'x': 'Artista', 'y': 'N√∫mero de √âxitos'},\n",
        "    text=top_artists.values,\n",
        "    color=top_artists.values,\n",
        "    color_continuous_scale='Viridis'\n",
        ")\n",
        "\n",
        "# Ajustar la visualizaci√≥n del gr√°fico\n",
        "fig.update_traces(textposition='outside', texttemplate='%{text}', marker=dict(line=dict(width=1, color='black')))\n",
        "fig.update_layout(\n",
        "    xaxis=dict(title='Artista', tickangle=45),\n",
        "    yaxis_title='N√∫mero de √âxitos',\n",
        "    title_font_size=20,\n",
        "    title_x=0.5,\n",
        "    coloraxis_showscale=False,\n",
        "    plot_bgcolor='white',\n",
        "    showlegend=False\n",
        ")\n",
        "\n",
        "# Mostrar el gr√°fico\n",
        "fig.show()\n"
      ]
