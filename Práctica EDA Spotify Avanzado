        "# Simular valores faltantes en la columna 'in_spotify_charts'\n",
        "df_spotify_2023.loc[5:10, 'in_spotify_charts'] = None\n",
        "\n",
        "# Rellenar valores faltantes\n",
        "df_spotify_2023['in_spotify_charts_ffill'] = df_spotify_2023['in_spotify_charts'].fillna(method='ffill')\n",
        "df_spotify_2023['in_spotify_charts_bfill'] = df_spotify_2023['in_spotify_charts'].fillna(method='bfill')\n",
        "\n",
        "# Mostrar los resultados\n",
