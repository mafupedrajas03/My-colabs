      },
      "source": [
        "filtro = df_titanic[\"Sex\"] == \"female\"\n",
        "\n",
        "num_mujeres = filtro.sum()\n",
        "total_pasajeros = len(df_titanic)\n",
        "prob_ser_mujer = num_mujeres / total_mujeres\n",
        "\n",
        "print(f\"Número de mujeres: {num_mujeres}\")\n",
        "print(f\"Total mujeres: {total_pasajeros}\")\n",
