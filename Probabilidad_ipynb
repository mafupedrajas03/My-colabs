      "source": [
        "survival_rate_child = df_titanic[df_titanic['is_child'] == True]['Survived'].mean()\n",
        "survival_rate_adult = df_titanic[df_titanic['is_child'] == False]['Survived'].mean()\n",
        "diff_obs = survival_rate_child - survival_rate_adult\n",
        "\n",
        "print(f\"Survival rate for children (Age < 18): {survival_rate_child:.4f}\")\n",
        "print(f\"Survival rate for adults (Age >= 18): {survival_rate_adult:.4f}\")\n",
        "print(f\"Observed difference in survival rates: {diff_obs:.4f}\")"
      ],
      "execution_count": 45,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Survival rate for children (Age < 18): 0.5398\n",
            "Survival rate for adults (Age >= 18): 0.3612\n",
            "Observed difference in survival rates: 0.1786\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def funcion_obtener_media(df, col_vector, target_name):\n",
        "    df[\"groupbycol\"] = col_vector\n",
        "    stats = df.groupby(\"groupbycol\")[target_name].mean()\n",
        "    data = {}\n",
        "    for g, v in stats.items():\n",
        "        data[f\"{target_name}_mean_{g}\"] = v\n",
        "    return pd.DataFrame([data])"
      ],
      "metadata": {
        "id": "poTGCQC0_GMF"
      },
      "execution_count": 46,
      "outputs": []
    },
