    {
      "cell_type": "code",
      "source": [
        "def simular_lanzamiento_moneda(tiradas):\n",
        "    df_i = pd.DataFrame(\n",
        "        [np.random.choice([1,0], tiradas)],\n",
        "        columns=[f\"Ronda {i}\" for i in range(tiradas)]\n",
        "        )\n",
        "    return df_i"
      ],
      "metadata": {
        "id": "JmVfnk0kW_nU"
      },
      "execution_count": 22,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def escenario_simulacion(num_simulaciones, funcion_sim):\n",
        "    df_sim = pd.DataFrame()\n",
        "    for _ in range(num_simulaciones):\n",
        "        df_sim_i = funcion_sim()\n",
        "        df_sim = pd.concat([df_sim, df_sim_i], ignore_index=True)\n",
        "    return df_sim"
      ],
      "metadata": {
        "id": "iRclOg3aCYKI"
      },
      "execution_count": 23,
      "outputs": []
    },
